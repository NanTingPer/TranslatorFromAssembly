<UserControl
    x:Class="TranslatorFromAssembly.Views.ListView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:app="using:TranslatorFromAssembly"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <!--  OpenPaneLength="{Binding Source={x:Static app:App.PaneSiezs[0]}}  -->
    <SplitView IsPaneOpen="{Binding IsPaneOpen}" OpenPaneLength="200">
        <SplitView.Content>
            <Grid RowDefinitions="30,*">
                <Button Command="{Binding IsPaneOpenMethodCommand}" Content="run" />
            </Grid>
        </SplitView.Content>

        <SplitView.Pane>
            <Button Command="{Binding IsPaneOpenMethodCommand}" Content="run" />
        </SplitView.Pane>

    </SplitView>
</UserControl>
